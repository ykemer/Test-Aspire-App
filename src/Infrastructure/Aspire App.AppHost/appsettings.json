{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Aspire": {
    "Hosting": {
      "Https": {
        "EnableForAllEndpoints": true,
        "CertificateMode": "Development"
      }
    },
    "Microsoft": {
      "EntityFrameworkCore": {
        "SqlServer": {
          "ConnectionString": "Server=(localdb)\\MSSQLLocalDb;Database=StudentsDB;Integrated Security=true;",
          "DbContextPooling": true,
          "HealthChecks": false,
          "Tracing": false,
          "Metrics": true
        }
      }
    }
  }
}
